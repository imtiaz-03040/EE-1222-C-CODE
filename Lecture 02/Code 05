// Author Name: Md. Imtiaz Ahmed
// Department:EEE,KUET
// Date:September 2, 2025.
#include<stdio.h>
void main ()
{
   float a1,a2,b1,b2,c1,c2,I1,I2,D,D1,D2;
   printf("Enter the value of a1,a2,b1,b2,c1,c2-");
   scanf("%f%f%f%f%f%f",&a1,&a2,&b1,&b2,&c1,&c2);

     D=(a1*b2)-(a2*b1);
    if(D==0)
    {
        printf("The equation has no unique solution.\n");
    }
    else
    {
        D1=(c1*b2)-(c2*b1);
        D2=(a1*c2)-(a2*c1);

        I1=D1/D;
        I2=D2/D;

        printf("Solution:\n");
        printf("I1 = %.2f A\n", I1);
        printf("I2 = %.2f A\n", I2);

    }

}
