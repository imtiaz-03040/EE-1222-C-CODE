// Program Name:  Find Largest & Smallest Number using conditional operator
// Author Name: Md. Imtiaz Ahmed
// Department:EEE,KUET
// Date:September 9, 2025.
#include<stdio.h>
void main ()
{
    int a,b,c,d,e,f,g;
    printf("Enter the value of a,b,c:");
    scanf("%d%d%d",&a,&b,&c);

   d=(a>b?a:b);
   e=(c>d?c:d);
   printf("The largest number is =%d\n",e);

   f=(a<b?a:b);
   g=(c<f?c:f);
   printf("The smallest number is =%d",g);
}
